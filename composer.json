{
    "name": "innmind/math",
    "type": "library",
    "description": "Library holding a set of math functions",
    "keywords": ["math"],
    "homepage": "http://github.com/Innmind/Math",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Math/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/immutable": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Math\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Math\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/coding-standard": "~2.0"
    }
}
