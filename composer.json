{
    "name": "innmind/math",
    "type": "library",
    "description": "Library holding a set of math functions",
    "keywords": ["math"],
    "homepage": "http://github.com/Innmind/Math",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Math/issues"
    },
    "require": {
        "php": "~7.0",
        "innmind/immutable": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Math\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Math\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
